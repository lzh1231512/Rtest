<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=0">
    <script src="~/js/loadJs.js" asp-append-version="true"></script>
    <script>
        loadStyle(['@Url.Content("~/lib/bootstrap/dist/css/bootstrap.css")', '@Url.Content("~/css/site.css")']);
        loadjs(['@Url.Content("~/lib/jquery/dist/jquery.js")', '@Url.Content("~/lib/bootstrap/dist/js/bootstrap.js")']);
    </script>
    <style>
        @@media (min-width: 1596px) {
            #div-body-content {
                width: 1596px;
            }
        }
    </style>
</head>
<body>
    <div id="div-body-content" class="container body-content">
        @RenderBody()
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>
